<div class="col-md-6 mx-auto text-center">
            <h1 class="wv-heading--title">
               Sign Up
            </h1>
            <h2 class="wv-heading--subtitle">
              Party rock is in the house toniiiiiiight
              <br>
              Everybody gon have a gooooooood time
                </h2>

      </div>
      <br>
<div class="row">
<div class="col-md-4 mx-auto">
    <div class="myform form ">
<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

  <div class="field form-group">
    <%= f.email_field :email, placeholder: :email, autofocus: true, autocomplete: "email",:class=>"form-control my-input" %>
  </div>
  <div class="field form-group">
    <%= f.text_field :username,placeholder: :username, autofocus: true, autocomplete: "username",:class=>"form-control my-input"%>
  </div>

  <div class="field form-group">
    <%= f.password_field :password, placeholder: "#{:password} (#{@minimum_password_length} characters minimum)",autocomplete: "new-password",:class=>"form-control my-input" %>
  </div>

  <div class="field form-group">
    <%= f.password_field :password_confirmation, placeholder: :password_confirmation,autocomplete: "new-password", :class=>"form-control my-input" %>
  </div>

  <div class="actions text-center">
    <%= f.submit "Create Your Free Account", :class=>"btn btn-block send-button tx-tfm"%>
  </div>
<% end %>
<div class="col-md-12 ">
                     <div class="login-or">
                        <hr class="hr-or">
                        <span class="span-or">or</span>
                     </div>
                  </div>
<div class="form-group">
<%= render "devise/shared/links" %>
</div>
</div>
</div>
</div>
